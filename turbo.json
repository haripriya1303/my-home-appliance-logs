{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["package-lock.json", "tsconfig.json"],
  "tasks": {
    "lint": {
      "outputs": [],
      "dependsOn": ["^lint"]
    },
    "test": {
      "outputs": [],
      "dependsOn": ["^test"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**", "*.tsbuildinfo"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^dev"]
    },
    "start": {
      "cache": false
    },
    "db:generate": {
      "cache": false
    },
    "db:migrate": {
      "cache": false
    },
    "db:seed": {
      "cache": false
    }
  }
}
